<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping package="org.hibernate.auction.model">

<class name="CustomerLocation"
	table="CUSTOMER_LOCATION">

	<!-- Common id property. -->
	<composite-id>
		<key-property name="one" column="CUSTOMER_LOC_ONE" length="10"/>
		<key-property name="two" column="CUSTOMER_LOC_TWO" length="10"/>
	</composite-id>

</class>

</hibernate-mapping>